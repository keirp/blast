#!/usr/bin/env bash

#SBATCH --gres=gpu:1
#SBATCH --mem=16G
#SBATCH -p t4v1,t4v2,p100
#SBATCH --cpus-per-task=12
#SBATCH --account=deadline
#SBATCH --qos=deadline
#SBATCH --exclude=gpu109,gpu090,gpu072,gpu077,gpu064

export MKL_SERVICE_FORCE_INTEL=1

COMMAND=$(python experiments/launcher/get_command.py --i=$SLURM_ARRAY_TASK_ID --jobs_file=$1 --checkpoint_path=/checkpoint/${USER}/${SLURM_JOB_ID})
echo "${COMMAND}"
export WANDB_RUN_GROUP="$2"
eval $COMMAND